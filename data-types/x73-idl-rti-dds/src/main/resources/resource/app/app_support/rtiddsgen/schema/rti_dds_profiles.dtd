<!-- dds -->
<!ELEMENT dds (configuration_variables|qos_library|types|type_library|domain_library|participant_library|domain_participant_library|application|transformation_library|adapter_library|plugin_library|routing_service)+>
<!ATTLIST dds xmlns:xsi CDATA #IMPLIED>
<!ATTLIST dds xsi:noNamespaceSchemaLocation CDATA #IMPLIED>
<!ATTLIST dds version CDATA "7.1.0">

<!-- domain_library -->
<!ELEMENT domain_library (domain)+>
<!ATTLIST domain_library name CDATA #REQUIRED>

<!-- domain -->
<!ELEMENT domain (register_type| 
				  topic| 
				  domain_id| 
				  participant_qos| 
				  deserialize_mode| 
				  type_config)*>
<!ATTLIST domain name CDATA #REQUIRED>
<!ATTLIST domain base_name CDATA #IMPLIED>
