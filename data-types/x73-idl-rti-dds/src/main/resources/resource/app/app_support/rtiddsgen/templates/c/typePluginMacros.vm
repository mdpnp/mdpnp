##-----------------------------------------------------------------------------
## Macro to print array of sequences
##-----------------------------------------------------------------------------
#macro(printArrayOfSeq $member)
{
    int i;
#*--*##if($member.isOptionalArray)
#*----*##declareMemberType($member)* elem = NULL;
    if (${member.sampleRef} == NULL) {
        RTICdrType_printIndent(indent_level+1);
        RTILogParamString_printPlain("$member.printName: NULL\n");
    } else {
        RTICdrType_printIndent(indent_level+1);
        RTILogParamString_printPlain("$member.printName:\n");
        elem = (#declareMemberType($member)*) (${member.sampleRef}[0]);
        for (i = 0; i < (int) ($member.arrayTotalSize); i++, elem++) {
#*--------*##printSeq($member "2" "elem" "(*elem)")
        }
    }
#*--*##else
    RTICdrType_printIndent(indent_level+1);
    RTILogParamString_printPlain("$member.printName:\n");
    for (i = 0; i < (int) ($member.arrayTotalSize); i++) {
#*--*##printSeq(
            $member
            "2"
            "${member.sampleRef}#if($member.dimensionList)[i]#end"
            "(*(${member.sampleRef}#if($member.dimensionList)[i]#end))")
    }
#**--*##end
}
#end

##-----------------------------------------------------------------------------
## Macro to print sequences
## - $member: the sequence member to print.
## - $indent: the current indentation level.
## - $seqSampleRef__: the reference to the sequence member.
## - $seqSample__: the sequence sample itself.
##-----------------------------------------------------------------------------
#macro(printSeq $member $indent $seqSampleRef__ $seqSample__)
#if(${member.isOptional} && !${member.isOptionalArray})
if ($seqSampleRef__ == NULL) {
    RTICdrType_printIndent(indent_level+ $indent);
#*--*##if($member.dimensionList)
    RTILogParamString_printPlain("%s[%d]: NULL\n","$member.printName",i);
#*--*##else
     RTILogParamString_printPlain("$member.printName: NULL\n");
#*--*##end
} else {
#end
#*--*##if($member.typeKind=="user")
    if (${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__) != NULL) {
        RTICdrType_printArray(
            ${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__),
            (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
            sizeof(${member.nativeTypeFQName}),
            (RTICdrTypePrintFunction)${member.nativeTypeFQName}PluginSupport_print_data,
            #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent);
    } else {
        RTICdrType_printPointerArray(
            ${member.nativeTypeSeqFQName}_get_discontiguous_bufferI($seqSampleRef__),
            (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
            (RTICdrTypePrintFunction)${member.nativeTypeFQName}PluginSupport_print_data,
            #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent);
    }

#*--*##elseif($member.strSize && ($envMap.stringIsObject && !$member.typeName.equals("wstring")))
#*----*##if($member.isPointer)
#*------*##set($sequenceElement="(*($seqSampleRef__))")
#*----*##else
#*------*##set($sequenceElement="${seqSample__}")
#*----*##end
#*----*##printStringObjectSeq(${sequenceElement} $member)
#*--*##elseif($member.strSize)
    if (${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__) != NULL) {
        RTICdrType_printStringArray(
            ${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__),
            (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
            #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent,
            $member.typeEnum);
    } else {
        RTICdrType_printStringPointerArray(
            ${member.nativeTypeSeqFQName}_get_discontiguous_bufferI($seqSampleRef__),
            (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
            #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent,
            $member.typeEnum);
    }

#*--*##else
    if (${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__) != NULL) {
        RTICdrType_printArray(
            ${member.nativeTypeSeqFQName}_get_contiguous_bufferI($seqSampleRef__),
            (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
            $member.elementSize,
            (RTICdrTypePrintFunction)$member.elementPrintMethod,
            #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent);
    } else {
            RTICdrType_printPointerArray(
                ${member.nativeTypeSeqFQName}_get_discontiguous_bufferI($seqSampleRef__),
                (RTICdrUnsignedLong) ${member.nativeTypeSeqFQName}_get_length($seqSampleRef__),
                (RTICdrTypePrintFunction)$member.elementPrintMethod,
                #if($member.dimensionList)"" #else "$member.printName"#end, indent_level + $indent);
    }


#*--*##end
#if(${member.isOptional} && !${member.isOptionalArray})
}
#*--*##end
#end

