##-----------------------------------------------------------------------------
## Macro to print builtins
##-----------------------------------------------------------------------------

#macro(printBuiltin $member)
#*--*##if($member.strSize && (!$envMap.stringIsObject || $member.typeName.equals("wstring")))
if (${member.sampleRef}==NULL) {
    ${member.elementPrintMethod}(
    NULL,"$member.printName", indent_level + 1);
} else {
    ${member.elementPrintMethod}(
    #if($member.isPointer)*#end${member.sampleRef},"$member.printName", indent_level + 1);    
}
#*--*##else

#*----*##if($member.strSize)

#*------*##if($member.isOptional || $member.isPointer)
if (${member.sampleRef}==NULL) {
    ${member.elementPrintMethod}(NULL,"$member.printName", indent_level + 1);
} else {
#*------*##end
    ${member.elementPrintMethod}(
        ${member.sample}${member.cStr},"$member.printName", indent_level + 1);
#*------*##if($member.isOptional || $member.isPointer)
}
#*------*##end

#*----*##else
    ${member.elementPrintMethod}(
        ${member.sampleRef}, "$member.printName", indent_level + 1);    
#*----*##end
#*--*##end
#end

