                <![CDATA[
                It can only be 255 characters in length.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="threadNamePrefix">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
