    <xs:simpleType name="snippetElementBaseName">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                A QoS Profile can inherit its values from other QoS Profiles
                using this attribute. Your can use either one built-in QoS
                profile or your own previously created profile.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="builtinSnippetProfileName">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:whiteSpace value="collapse"/>
                    <xs:pattern value="((::)?[a-zA-Z0-9_.])+"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

    <xs:simpleType name="elementBaseName">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                A QoS Profile can inherit its values from other QoS Profiles
                using this attribute. Your can use either one built-in QoS
                profile or your own previously created profile.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:union memberTypes="builtinProfileName">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:whiteSpace value="collapse"/>
                    <xs:pattern value="((::)?[a-zA-Z0-9_.])+"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

