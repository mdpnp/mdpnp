            <xs:enumeration value="BuiltinQosSnippetLib::Feature.FlowController.209Mbps"/>
            <xs:enumeration value="BuiltinQosSnippetLib::Feature.FlowController.52Mbps"/>
            <xs:enumeration value="BuiltinQosSnippetLib::Feature.AutoTuning.Enable"/>
            <xs:enumeration value="BuiltinQosSnippetLib::Feature.Monitoring.Enable"/>
            <xs:enumeration value="BuiltinQosSnippetLib::Feature.Monitoring2.Enable"/>
            <xs:enumeration value="BuiltinQosSnippetLib::Feature.Security.Enable"/>
