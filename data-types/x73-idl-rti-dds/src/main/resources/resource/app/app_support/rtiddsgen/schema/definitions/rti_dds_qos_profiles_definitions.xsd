    <!-- RTI extension -->
    <xs:simpleType name="compressionIdKind">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[
                Supported values (case-insensitive):
                - ZLIB
                - BZIP2
                - LZ4
                - MASK_ALL
                - MASK_NONE
                ]]>
            </xs:documentation>
        </xs:annotation>
