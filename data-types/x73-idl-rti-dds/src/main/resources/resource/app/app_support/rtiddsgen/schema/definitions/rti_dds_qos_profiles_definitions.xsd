    <xs:attributeGroup name="entityNameAttrGroup">
        <xs:attribute name="name"
                      type="resourceName"
                      use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en-US">
                    <![CDATA[
                    Entity configuration name. Uniquely identifies an element
                    in the configuration.
                    This name can be referrenced by other elements in the
                    configuration.
                    ]]>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>

    <xs:simpleType name="propertyName">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                The name of the Property.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="([a-zA-Z0-9_.:])+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="nameFilter">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                Type of of a regular expression string that represents a filter
                applied to a name, such as:
                - Topic name
                - Type name
                - Subset of data structure fields
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="([a-zA-Z0-9/\-*_.:\]\[@\s])+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="nameFilterList">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[
                 <xs:string>
                 ]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                Comma-separted list of name filters
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="([a-zA-Z\d/\-\*_\.:,\]\[\s])+"/>
        </xs:restriction>
    </xs:simpleType>

