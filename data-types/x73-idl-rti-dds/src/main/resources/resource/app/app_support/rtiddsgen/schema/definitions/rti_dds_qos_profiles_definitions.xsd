    <!-- RTI extension -->
    <xs:complexType name="domainParticipantWireProtocolQosPolicy">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[
                <<RTI Extension>>
                The WIRE_PROTOCOL QoS policy configures some Real-Time
                Publish Subscribe (RTPS) protocol-related properties for the
                DomainParticipant.

                The open RTPS protocol is used by RTI Connext to format and
                interpret messages between DomainParticipants.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="participant_id"
                        type="integerFromMinus1"
                        minOccurs="0"/>
            <xs:element name="rtps_host_id"
                        type="rtpsId"
                        minOccurs="0"/>
            <xs:element name="rtps_app_id"
                        type="rtpsId"
                        minOccurs="0"/>
            <xs:element name="rtps_instance_id"
                        type="rtpsId"
                        minOccurs="0"/>
            <xs:element name="rtps_well_known_ports"
                        type="rtpsWellKnownPorts"
                        minOccurs="0"/>
            <xs:element name="rtps_auto_id_kind"
                        type="wireProtocolQosPolicyAutoKind"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <![CDATA[
                        Supported only in RTI Connext DDS Professional and RTI Connext DDS Secure.
                        ]]>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rtps_reserved_port_mask"
                        type="rtpsReservedPortKindMask"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <![CDATA[
                        Supported only in RTI Connext DDS Professional and RTI Connext DDS Secure.
                        ]]>
                    </xs:documentation>
                    <xs:documentation>
                        <![CDATA[
                        Supported values:
                            - MASK_DEFAULT
                            - MASK_NONE
                            - MASK_ALL
                            - Or a combination of (BUILTIN_UNICAST|BUILTIN_MULTICAST|USER_UNICAST|USER_MULTICAST)
                        ]]>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="compute_crc"
                        type="xs:boolean"
                        minOccurs="0"/>
            <xs:element name="check_crc"
                        type="xs:boolean"
                        minOccurs="0"/>
            <xs:element name="require_crc"
                        type="xs:boolean"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <![CDATA[Supported only in RTI Connext DDS Micro.]]>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="computed_crc_kind"
                        type="computedCrcKind"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <![CDATA[Supported only in RTI Connext DDS Micro.]]>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="allowed_crc_mask"
