    <!-- =================================================================== -->
    <!-- Simple Types                                                        -->
    <!-- =================================================================== -->
    <xs:simpleType name="byteData">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                Uses a binary encoding in hexadecimal, each binary octet is
                coded as two characters.
                For example, a 4-byte stream: f3123241
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:hexBinary">
            <xs:maxLength value="2048"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="elementName">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[<xs:string>]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                Name that uniquely identifies this element.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:maxLength value="255"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Resource name type -->
    <xs:simpleType name="resourceName">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[&lt;xs:string&gt;]]>
            </xs:documentation>
            <xs:documentation>
                <![CDATA[
                Name that uniquely identifies a resource.
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="elementName"/>
    </xs:simpleType>

